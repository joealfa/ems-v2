{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Information",
        "HotChocolate": "Debug",
        "HotChocolate.Execution": "Debug",
        "System": "Information"
      }
    }
  },
  "Gateway": {
    "BaseUrl": "https://localhost:5003"
  },
  "ApiClient": {
    "BaseUrl": "https://localhost:7166"
  },
  "Redis": {
    "ConnectionString": "localhost:6379",
    "InstanceName": "EmsGateway_Dev:"
  },
  "Caching": {
    "DefaultTtlMinutes": 1,
    "EntityTtlMinutes": 2,
    "ListTtlMinutes": 1
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:5173",
      "https://localhost:5173"
    ]
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "Port": 5672,
    "VirtualHost": "ems",    
    "ExchangeName": "ems.events",
    "QueueName": "ems.gateway.cache-invalidation",
    "UseSsl": false,
    "RetryCount": 3,
    "RetryDelayMilliseconds": 1000,
    "Enabled": true
  }
}
