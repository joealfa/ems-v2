<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="localhost" value="http://host.docker.internal:5000/v3/index.json" allowInsecureConnections="true" />
  </packageSources>
  <apiKeys>
    <!-- Add the key for the specific source URL -->
    <add key="http://host.docker.internal:5000/v3/index.json" value="your-nuget-api-key" />
  </apiKeys>
</configuration>
